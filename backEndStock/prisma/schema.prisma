
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model products{
  id          Int      @id @default(autoincrement())
  name        String   
  quantity    Int
  price       Float
  category    String
  desc        String?
  date        DateTime @default(now())
}
